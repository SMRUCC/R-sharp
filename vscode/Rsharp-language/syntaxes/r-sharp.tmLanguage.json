{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "R# GCModeller Scripting Language",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings1"
		},
		{
			"include": "#strings2"
		},
		{
			"include": "#strings3"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "keyword.control.r-sharp",
				"match": "\\b(if|while|for|return|let|as|integer|string|double|boolean|object|imports|require|from|steps|which|loop|repeat|function|in|else|print)\\b"
			}]
		},
		"strings1": {
			"name": "string.quoted.double.r-sharp",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.r-sharp",
					"match": "\\\\."
				}
			]
		},
		"strings2": {
			"name": "string.quoted.double.r-sharp",
			"begin": "`",
			"end": "`",
			"patterns": [
				{
					"name": "constant.character.escape.r-sharp",
					"match": "\\\\."
				}
			]
		},
		"strings3": {
			"name": "string.quoted.double.r-sharp",
			"begin": "'",
			"end": "'",
			"patterns": [
				{
					"name": "constant.character.escape.r-sharp",
					"match": "\\\\."
				}
			]
		},
		"comments": {
			"name": "comment.line.r-sharp",
			"begin": "#",
			"end": "\n"
		}
	},
	"scopeName": "main.r"
}