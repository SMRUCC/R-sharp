
Imports System.Text
''' <summary>
''' 生成数据类型的帮助文档
''' </summary>
Module Type

    Public Sub GenerateDocument(repository As String)
        Dim index As New StringBuilder(My.Resources.index)

        Call index.Replace("=""assets", "=""../assets")
        Call index.SaveTo($"{repository}/TypeLinks/index.html", System.Text.Encoding.UTF8)
    End Sub
End Module
