Imports System.Text
Imports Microsoft.VisualBasic.Scripting.ShoalShell

Module Contributors

    Public Sub GenerateDocument(repository As String, library As SPM.Nodes.Namespace())
        Dim index As New StringBuilder(My.Resources.index)

        Call index.SaveTo($"{repository}/contributors.html", System.Text.Encoding.UTF8)
    End Sub
End Module
